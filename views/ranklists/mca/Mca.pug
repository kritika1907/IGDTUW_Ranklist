include ../../common/Header
link(rel='stylesheet', href='/css/Mca.css')
body
  #message-box-modal.modal
   .modal-content
    .modal-subcontent.bg-black
    div.close(onclick="utils.CloseMessageBox()") &times;
    .modal-subcontent
     p#message-box-content
     div.d-flex.justify-content-center
      button(class= "custom-btn modal-btn" onclick="utils.CloseMessageBox()") 
       span OK
  #top-nav-bar
    h2 Rank List
    div
      ul.nav-links
        li  
          a(href="/logout") Logout
  #loader.loader
  .d-flex.jutify-content-center.h-90.flex-direction-column
   .row
      .col-12#search-tab
          .wrapper.w-80.m-auto
            h3.text-align-center MCA Ranklist
            br
            .selections
              p.text-align-center
                sub
                  strong College:
                  |  Indira Gandhi Delhi Technical University For Women
              p.text-align-center
                sub
                  strong Course:
                  |  MCA
            form.header-form(novalidate="")
              .form-group
                select.form-control#college(name="college" required="")
                  option(value="IGDTUW") IGDTUW
              .form-group
                select.form-control#batch(name="batch" required="")
                  option(value="15") 2015 - 2018
                  option(value="16") 2016 - 2019
                  option(value="17") 2017 - 2020
                  option(value="18") 2018 - 2021
                  option(value="19") 2019 - 2022
                  option(value="20") 2020 - 2022
                  option(value="21") 2021 - 2023
                  option(value="22") 2022 - 2024
                  option(value="23") 2023 - 2025
              .form-group
                select.form-control#sem(name="sem" required="")
                  option(value="0") Overall
                  option(value="1") Sem 1
                  option(value="2") Sem 2
                  option(value="3") Sem 3
                  option(value="4") Sem 4
              button.btn.btn-dark(style="color: white;") Search
          br
      .col#table-area.d-none
          .wrapper.w-80.m-auto
script(src='/vendors/apexcharts/dist/apexcharts.min.js')
script(src="/js/ranklists/Mca.js")